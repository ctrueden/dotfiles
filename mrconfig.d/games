# Game-related repositories

[code/games/Ikemen-GO]
checkout = git clone git@github.com:ikemen-engine/Ikemen-GO

[code/games/jeopardy]
checkout = git clone git@github.com:pfroud/jeopardy && git remote add ctrueden git@github.com:ctrueden/jeopardy && git fetch ctrueden

[code/games/pbnsolve]
checkout = git clone git@github.com:avi-levy/pbnsolve

[code/games/srs-game]
checkout = git clone git@github.com:ekjellman/srs-game

[code/games/supermariowar]
checkout = git clone --recursive git@github.com:mmatyas/supermariowar
